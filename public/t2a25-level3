<script>
    const char = document.getElementById("myChar");
    let myVertArc = 0;
    let myVertHeight = 0;
addEventListener("keydown", event => {
    if (event.key === "ArrowUp") {
 myVertHeight = 0;
let myCharTop = parseInt(char.style.top)
 let myUINT = setInterval(() => {
    myVertArc -=0.5;
    myVertHeight +=1
    char.style.top = myVertArc + myCharTop + "%";
    if (myVertHeight === 10) {
        clearInterval(myUINT);
    }
    }, 10);
}
}, 100);

</script>
